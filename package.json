    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "gravatar": "^1.8.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.9.22",
    "react-bootstrap": "^1.2.2",
    "react-modal": "^3.11.2",
    "react-moment": "^0.9.7",
